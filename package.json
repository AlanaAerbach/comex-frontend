{
    "name": "comex-frontend",
    "version": "1.0.0",
    "description": "Projeto frontend do melhor ecommerce do Brasil: Comex!",
    "main": "index.js",
    "type": "module",
    "@local/uuidv4": "file:./node_modules/uuid/dist/esm-browser/index.js",
    "scripts": {
        "test": "jest",
        "test:essencial": "jest -t Essencial",
        "test:desejavel": "jest -t Desejavel",
        "test:desafio": "jest -t Desafio",
        "test:watch": "jest --watchAll",
        "api:fake": "json-server --watch db.json",
        "server": "lite-server --baseDir=dist",
        "start": "concurrently \"npm run watch\" \"npm run server\"",
        "compile": "tsc",
        "watch": "tsc -w"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/level-up-obx/comex-frontend.git"
    },
    "keywords": [
        "onebox",
        "alura",
        "level-up",
        "javascript",
        "vuejs"
    ],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/level-up-obx/comex-frontend/issues"
    },
    "homepage": "https://github.com/level-up-obx/comex-frontend#readme",
    "devDependencies": {
        "@types/uuid": "^9.0.1",
        "concurrently": "^6.0.0",
        "lite-server": "^2.6.1",
        "moment": "^2.24.0",
        "typescript": "^4.2.2",
        "uuid": "^9.0.0",
        "uuidv4": "^6.2.13"
    },
    "dependencies": {
        "json-server": "^0.17.2"
    }
}
